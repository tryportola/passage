{
  "name": "@portola/passage",
  "version": "1.0.3",
  "description": "Official TypeScript SDK for the Passage API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "passage",
    "portola",
    "fintech",
    "lending",
    "api",
    "sdk",
    "typescript",
    "neobank",
    "lender"
  ],
  "author": "Portola <hello@tryportola.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tryportola/passage-sdk.git",
    "directory": "packages/passage"
  },
  "homepage": "https://docs.tryportola.com",
  "bugs": {
    "url": "https://github.com/tryportola/passage-sdk/issues"
  },
  "dependencies": {
    "axios": "^1.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
